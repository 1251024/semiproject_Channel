<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<properties resource="channel/db_lyj/db.properties"></properties>
	
	<typeAliases>
		<typeAlias type="channel.lyj_member.MemberDto" alias="memberDto"/>
		<typeAlias type="channel.lyj_room.RoomDto" alias="roomDto"/>
		<typeAlias type="channel.lyj_room.RoomMemberDto" alias="roomMemberDto"/>
		<typeAlias type="channel.lyj_chat.ChatDto" alias="chatDto"/>
	</typeAliases>
		
  	<environments default="development">
    	<environment id="development">
      	<transactionManager type="JDBC"/>
	      	<dataSource type="POOLED">
	        	<property name="driver" value="${driver}"/>
	        	<property name="url" value="${url}"/>
	        	<property name="username" value="${username}"/>
	        	<property name="password" value="${password}"/>
	      	</dataSource>
    	</environment>
  	</environments>

	<mappers>	
		<mapper resource="channel/db_lyj/channel-mapper-member.xml"/>
		<mapper resource="channel/db_lyj/channel-mapper-room.xml"/>
		<mapper resource="channel/db_lyj/channel-mapper-chat.xml"/>
	</mappers>


</configuration>