<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<properties resource="channel/db/db.properties"></properties>
	
	<typeAliases>
		<typeAlias type="channel.member.dto.MemberDto" alias="memberDto"/>
		<typeAlias type="channel.room.dto.RoomDto" alias="roomDto"/>
		<typeAlias type="channel.room.dto.RoomMemberDto" alias="roomMemberDto"/>
		<typeAlias type="channel.chat.dto.ChatDto" alias="chatDto"/>
	</typeAliases>
		
  	<environments default="development">
    	<environment id="development">
      	<transactionManager type="JDBC"/>
	      	<dataSource type="POOLED">
	        	<property name="driver" value="${driver}"/>
	        	<property name="url" value="${url}"/>
	        	<property name="username" value="${username}"/>
	        	<property name="password" value="${password}"/>
	      	</dataSource>
    	</environment>
  	</environments>

	<mappers>
		
		<mapper resource="channel/db/channel-mapper.xml"/>
		
	</mappers>


</configuration>